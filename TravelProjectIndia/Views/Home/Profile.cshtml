    <script>

        function togglePasswordVisibility(inputId, iconId) {
            const passwordInput = document.getElementById(inputId);
            const passwordIcon = document.getElementById(iconId);

            if (passwordInput.type === 'password') {
                passwordInput.type = 'text';
                passwordIcon.className = 'toggle-password1 fa fa-eye';
            }
            else {
                passwordInput.type = 'password';
                passwordIcon.className = 'toggle-password1 fa fa-eye-slash';

            }
        }

        var flag = false;

        function checkPasswordsMatch() {
            const newPassword1 = document.getElementById('newPassword1');
            const newPassword2 = document.getElementById('newPassword2');


            if (newPassword1.value !== "" && newPassword2.value !== "") {
                if (newPassword1.value === newPassword2.value) {
                    const imageElement = document.getElementById('passwordLogo');
                    imageElement.src = '/img/currectpass.png';
                    flag = true;
                }
                else {
                    const imageElement = document.getElementById('passwordLogo');
                    imageElement.src = '/img/wrongpass.png';
                    flag = false;
                }
            }
            else if (newPassword2.value !== "") {
                const imageElement = document.getElementById('passwordLogo');
                imageElement.src = '/img/wrongpass.png';
                flag = false;
            }
            else {
                const imageElement = document.getElementById('passwordLogo');
                imageElement.src = '';
                flag = false;
            }
        }

        function ChangePassword() {
            if (flag) {

                const newPassword = document.getElementById('oldPassword').value;
                const newPassword2 = document.getElementById('newPassword2').value;

                if (newPassword != newPassword2) 
                {
                    alert("Password changed..")
                }
                else {
                    alert("Old Password and new password not be same..")
                }
            }
            else {
                alert("Both new password must be same.");
            }
        }
        // Add event listeners to all eye icons
        document.getElementById('oldPasswordEye').addEventListener('click', function () {
            togglePasswordVisibility('oldPassword', 'oldPasswordEye');
        });

        document.getElementById('newPassword1Eye').addEventListener('click', function () {
            togglePasswordVisibility('newPassword1', 'newPassword1Eye');
        });

        document.getElementById('newPassword2Eye').addEventListener('click', function () {
            togglePasswordVisibility('newPassword2', 'newPassword2Eye');
        });

        const password1 = document.getElementById('newPassword1');
        const password2 = document.getElementById('newPassword2');
        password1.addEventListener('input', checkPasswordsMatch);
        password1.addEventListener('change', checkPasswordsMatch);
        password2.addEventListener('input', checkPasswordsMatch);
        password2.addEventListener('change', checkPasswordsMatch);





    </script>
