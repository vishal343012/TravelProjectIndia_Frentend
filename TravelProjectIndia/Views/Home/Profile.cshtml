@section Scripts {
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.8.2/angular.min.js"></script>
    <script>
        var app = angular.module('ProfileDetail', []);

        app.controller('ProfileController', ['$scope', '$http', function ($scope, $http) {

            $scope.userData = {};
            $scope.userID = JSON.parse(sessionStorage.getItem('UserId'));
            $scope.userID += "";



            // Function to fetch user data
            $scope.getUserData = function () {
                // Request body
                var requestBody = {
                    "EmailId": $scope.userID
                };

                // API endpoint
                var apiUrl = 'https://nextholidaysinadiaapi.nextholidays.co.in/User/GetUserData';

                // Make POST request to fetch user data
                $http.post(apiUrl, requestBody)
                    .then(function (response) {
                        // On success, store user data in scope variable
                        $scope.userData = response.data;
                        console.log('user data',$scope.userData);
                        // Ensure DOM updates are applied
                        fetchCountryList();
                        setTimeout(function () {
                            document.getElementById('country-name').value = $scope.userData.countryId;
                            document.getElementById('country-code').value = $scope.userData.countryCode;
                            document.getElementById('gender').value = $scope.userData.gender;
                            document.getElementById('country-name').value = $scope.userData.countryId;
                            document.getElementById('city-name').value = $scope.userData.cityId;
                            if ($scope.userData.dob != null) {
                                document.getElementById('birth-date').value = $scope.userData.dob;
                                console.log($scope.userData.dob);
                            }
                            
                            

                        }, 5000);
                    })
                    .catch(function (error) {
                        console.error('Error fetching user data:', error);
                    });
            };

            
            if ($scope.userID !== null) {
                $scope.getUserData();
            }
            else {
                window.location.href = "/";
            }
        }]);
    </script>


    @* Edit profile *@

    <script>
        const firstName = document.getElementById('first-name');
        const firstNamev = firstName.value;

        const LastName = document.getElementById('last-name');
        const LastNamev = LastName.value;

        const email = document.getElementById('email-id');
        const emailv = email.value;

        const countryCode = document.getElementById('country-code');
        const countryCodev = countryCode.value;

        const mobileNumber = document.getElementById('mobile-number');
        const mobileNumberv = mobileNumber.value;

        const gender = document.getElementById('gender').value;
        const genderv = gender.value;

        const countryName = document.getElementById('country-name');
        const countryNamev = countryName.value;

        const cityName = document.getElementById('city-name');
        const cityNamev = cityName.value;

        const birthDate = document.getElementById('birth-date');
        const birthDatev = birthDate.value;

        document.addEventListener('DOMContentLoaded', function () {
            fetchCountryList();

        });



        function fetchCityData(countryId) {
            const apiUrl = `https://nextholidaysinadiaapi.nextholidays.co.in/User/GetCityList?CountryId=${countryId}`;
            fetch(apiUrl)
                .then(response => response.json())
                .then(data => {
                    const selectElement = document.getElementById('city-name');

                    selectElement.innerHTML = '<option selected disabled>Select a city</option>';

                    data.forEach(city => {
                        const option = document.createElement('option');
                        option.value = city.ID;
                        option.textContent = city.Name;
                        selectElement.appendChild(option);
                    });
                })
                .catch(error => {
                    console.error('Error fetching city data:', error);
                });
        }


        function fetchCountryList() {
            const url = 'https://nextholidaysinadiaapi.nextholidays.co.in/User/GetCountryList';

            fetch(url)
                .then(response => {
                    if (!response.ok) {
                        throw new Error('Network response was not ok ' + response.statusText);
                    }
                    return response.json();
                })
                .then(data => {

                    const selectElement = document.getElementById('country-name');
                    const countryCode = document.getElementById('country-code');

                    data.forEach(country => {
                        const option = document.createElement('option');
                        option.value = country.ID;
                        option.textContent = country.Name;
                        selectElement.appendChild(option);

                        const Codeoption = document.createElement('option');
                        Codeoption.value = country.MobileCode;
                        Codeoption.textContent = '+' + country.MobileCode;
                        countryCode.appendChild(Codeoption);

                    });
                })
                .catch(error => {
                    console.error('There was a problem with the fetch operation:', error);
                });
        }




    </script>



